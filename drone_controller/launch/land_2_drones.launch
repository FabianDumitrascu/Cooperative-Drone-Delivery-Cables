<launch>
    <!-- Default arguments -->
    <arg name="drone_name_1" default="flycrane"/>
    <arg name="drone_name_2" default="$(arg drone_name_1)1"/>

    <!-- Node forTakeoff-->
    <group ns = "BEP">
        <node name="land"
            pkg="drone_controller"
            type="land_node"
            output="screen">
            <param name="drone_name_1" value="$(arg drone_name_1)" />
            <param name="drone_name_2" value="$(arg drone_name_2)" />
        </node>
    </group>
</launch>

